{
  "name": "@automattic/vip-go",
  "version": "1.1.0",
  "description": "Node package helpers for the VIP Go platform",
  "main": "./src/index.js",
  "scripts": {
    "build": "echo \"No build step\"",
    "cmd:format": "prettier '**/*.(js|json|jsx|md|ts|tsx|yml|yaml)'",
    "cmd:lint": "eslint --ext 'js,jsx,ts,tsx' --quiet",
    "format": "npm run cmd:format -- --write",
    "format:check": "npm run cmd:format -- --check",
    "jest": "jest",
    "lint": "npm run cmd:lint .",
    "lint:fix": "npm run cmd:lint . -- --fix",
    "test": "npm run lint && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/vip-go-node.git"
  },
  "jest": {
    "verbose": true
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Automattic/vip-go-node/issues"
  },
  "homepage": "https://github.com/Automattic/vip-go-node#readme",
  "dependencies": {
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@automattic/eslint-plugin-wpvip": "0.5.8",
    "eslint": "8.45.0",
    "express": "4.18.2",
    "ioredis": "5.3.2",
    "jest": "29.6.1",
    "prettier": "npm:wp-prettier@2.8.5",
    "supertest": "6.3.3",
    "winston-transport": "4.5.0"
  }
}
